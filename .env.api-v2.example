# Node environment
NODE_ENV=development

# API server configuration
API_PORT=80
API_URL=http://localhost

# Database configuration
DATABASE_READ_URL=postgresql://postgres:postgres@database:5432/calendso
DATABASE_WRITE_URL=postgresql://postgres:postgres@database:5432/calendso
DATABASE_URL=postgresql://postgres:postgres@database:5432/calendso
DATABASE_HOST=database
POSTGRES_DB=calendso
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Redis configuration
REDIS_URL=redis://redis:6379

# Authentication secrets
NEXTAUTH_SECRET=secret
JWT_SECRET=secret

# Logging
LOG_LEVEL=info

# Sentry error tracking
SENTRY_DSN=
# KEEP THIS EMPTY, DISABLE SENTRY CLIENT INSIDE OF LIBRARIES USED BY APIv2
NEXT_PUBLIC_SENTRY_DSN=

# Stripe Billing
STRIPE_PRICE_ID_STARTER=
STRIPE_PRICE_ID_STARTER_OVERAGE=
STRIPE_PRICE_ID_ESSENTIALS=
STRIPE_PRICE_ID_ESSENTIALS_OVERAGE=
STRIPE_PRICE_ID_ENTERPRISE=
STRIPE_PRICE_ID_ENTERPRISE_OVERAGE=
STRIPE_API_KEY=
STRIPE_WEBHOOK_SECRET=

# Application URLs
WEB_APP_URL=http://localhost:3000
CALCOM_LICENSE_KEY=
GET_LICENSE_KEY_URL=

# API Key configuration
API_KEY_PREFIX=cal_

# Testing
IS_E2E=false

# Documentation
DOCS_URL=

# Axiom logging
AXIOM_DATASET=
AXIOM_TOKEN=

# Logger level
# DEBUG: 0
# INFO: 1
# WARN: 2
# ERROR: 3
LOGGER_BRIDGE_LOG_LEVEL="1"

# API version rewriting
# 1: Rewrite /api/v2 to /v2
# 0: Don't rewrite
REWRITE_API_V2_PREFIX=1

# Rate limiting configuration
RATE_LIMIT_DEFAULT_TTL_MS=60000
RATE_LIMIT_DEFAULT_LIMIT_API_KEY=100
RATE_LIMIT_DEFAULT_LIMIT_OAUTH_CLIENT=100
RATE_LIMIT_DEFAULT_LIMIT_ACCESS_TOKEN=100
RATE_LIMIT_DEFAULT_LIMIT=100
RATE_LIMIT_DEFAULT_BLOCK_DURATION_MS=600000

# Stripe team billing
STRIPE_TEAM_MONTHLY_PRICE_ID=
IS_TEAM_BILLING_ENABLED=true

